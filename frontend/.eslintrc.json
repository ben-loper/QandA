{
    "extends": [
        "react-app",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", {"endOfLine": "auto"}],
        "quotes": [2, "single", "avoid-escape"],
        "no-debugger": "warn",
        "no-console": "warn"
    }
}